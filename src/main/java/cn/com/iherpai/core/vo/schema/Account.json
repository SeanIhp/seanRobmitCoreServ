			]
		},
		"pageKeys": {
			"$id": "#/properties/pageKeys",
			"type": "array",
			"title": "The Pagekeys Schema",
			"default": "",
			"examples": [
				"\"aa\",\"bb\",\"cc\""
			],
			"uniqueItems": true
		},
		"pageNo": {
			"$id": "#/properties/pageNo",
			"type": "integer",
			"title": "The Pageno Schema",
			"default": 1,
			"examples": [
				1
			],
			"minimum": 1.0
		},
		"pageSize": {
			"$id": "#/properties/pageSize",
			"type": "integer",
			"title": "The Pagesize Schema",
			"default": 20,
			"examples": [
				10
			],
			"minimum": 5.0,
			"maximum": 200.0
		},
		"pageOrder": {
			"$id": "#/properties/pageOrder",
			"type": "array",
			"title": "The Pageorder Schema",
			"default": null,
			"items": {
				"$id": "#/properties/pageOrder/items",
				"type": "object",
				"title": "The Items Schema",
				"required": [
					"key",
					"isAsc"
				],
				"properties": {
					"key": {
						"$id": "#/properties/pageOrder/items/properties/key",
						"type": "string",
						"title": "The Key Schema",
						"default": "",
						"examples": [
							"username"
						],
						"pattern": "^(.*)$"
					},
					"isAsc": {
						"$id": "#/properties/pageOrder/items/properties/isAsc",
						"type": "boolean",
						"title": "The Isasc Schema",
						"default": true,
						"examples": [
							false
						]
					}
				}
			}
		},
		"pageCond": {
			"$id": "#/properties/pageCond",
			"type": "array",
			"title": "The Pagecond Schema",
			"default": null,
			"items": {
				"$id": "#/properties/pageCond/items",
				"type": "object",
				"title": "The Items Schema",
				"required": [
					"key",
					"calc",
					"value"
				],
				"properties": {
					"key": {
						"$id": "#/properties/pageCond/items/properties/key",
						"type": "string",
						"title": "The Key Schema",
						"default": "",
						"examples": [
							"type"
						],
						"pattern": "^(.*)$"
					},
					"calc": {
						"$id": "#/properties/pageCond/items/properties/calc",
						"type": "string",
						"title": "The Calc Schema",
						"default": "",
						"examples": [
							"eq"
						],
						"pattern": "^(.*)$"
					},
					"value": {
						"$id": "#/properties/pageCond/items/properties/value",
						"type": "integer",
						"title": "The Value Schema",
						"default": 0,
						"examples": [
							1
						]
					}
				}
			}
		},
		"pageCondMode": {
			"$id": "#/properties/pageCondMode",
			"type": "integer",
			"title": "The Pagecondmode Schema",
			"default": 1,
			"examples": [
