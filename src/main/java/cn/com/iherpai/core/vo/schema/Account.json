{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "object",
	"title": "The Root Schema",
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "The Id Schema",
			"default": 1,
			"examples": [
				100000000
			],
			"minimum": 0.0
		},
		"nid": {
			"$id": "#/properties/nid",
			"type": "integer",
			"title": "The Nid Schema",
			"default": 1,
			"examples": [
				1
			],
			"minimum": 0.0
		},
		"sid": {
			"$id": "#/properties/sid",
			"type": "string",
			"title": "The Sid Schema",
			"default": "",
			"examples": [
				"AABB_CCDD00"
			],
			"minLength": 64,
			"maxLength": 64,
			"pattern": "^\\w+$",
			"format": "regex"
		},
		"username": {
			"$id": "#/properties/username",
			"type": "string",
			"title": "The Username Schema",
			"default": "anonymous",
			"examples": [
				"superman"
			],
			"minLength": 3,
			"maxLength": 32,
			"pattern": "^[a-zA-Z0-9_\\u4e00-\\u9fa5]+$",
			"format": "regex"
		},
		"password": {
			"$id": "#/properties/password",
			"type": "string",
			"title": "The Password Schema",
			"default": "anonymous",
			"examples": [
				"aabbcc"
			],
			"pattern": "^(.*)$"
		},
		"question": {
			"$id": "#/properties/question",
			"type": "string",
			"title": "The Question Schema",
			"default": "",
			"examples": [
				"how old a u"
			],
			"pattern": "^(.*)$"
		},
		"answer": {
			"$id": "#/properties/answer",
			"type": "string",
			"title": "The Answer Schema",
			"default": "",
